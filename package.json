{
  "name": "filestack-adaptive",
  "author": "Filestack (https://filestack.com)",
  "version": "0.2.7",
  "description": "HTML5 picture elements powered by Filestack",
  "main": "build/main/index.js",
  "module": "build/module/index.js",
  "browser": "build/browser/index.umd.js",
  "typings": "build/main/index.d.ts",
  "repository": "https://github.com/filestack/adaptive",
  "scripts": {
    "build": "rm -rf build && npm run build:main && npm run build:module && npm run build:browser",
    "build:main": "tsc -p tsconfig.json",
    "build:module": "tsc -p config/tsconfig.module.json",
    "build:browser": "rollup -c config/rollup.config.js",
    "build:cdn": "NODE_ENV=production rollup -c config/rollup.config.js && node scripts/manifest.js",
    "build:test": "tsc -p tsconfig.json --inlineSourceMap",
    "docs": "rm -rf build/docs && typedoc src",
    "docs:publish": "npm run docs && gh-pages -d build/docs",
    "lint": "tslint --project .",
    "publish": "npm run build && npm run build:cdn && node scripts/publish.js",
    "pretest": "rm -rf build && npm run build:test",
    "release": "npm test && npm run build && standard-version",
    "test": "npm run lint && tsc && nyc mocha"
  },
  "license": "Apache-2.0",
  "keywords": [
    "filestack",
    "responsive images",
    "adaptive images",
    "picture",
    "pictures",
    "react",
    "ssr",
    "mobile web",
    "html5",
    "html picture"
  ],
  "dependencies": {
    "filestack-js": "^2.0.7",
    "hyperx": "^2.5.4",
    "nanohtml": "^1.6.1",
    "tslib": "^1.9.3"
  },
  "devDependencies": {
    "@babel/core": "^7.4.4",
    "@babel/preset-env": "^7.4.4",
    "@istanbuljs/nyc-config-typescript": "^0.1.3",
    "@types/mocha": "^5.2.6",
    "@types/node": "^11.13.8",
    "aws-sdk": "^2.444.0",
    "babel-plugin-minify-dead-code-elimination": "^0.5.0",
    "commitizen": "^3.1.1",
    "cz-conventional-changelog": "^2.1.0",
    "fs-jetpack": "^2.2.2",
    "gh-pages": "^2.0.1",
    "html-validator": "^4.0.2",
    "husky": "^2.1.0",
    "mime-types": "^2.1.24",
    "mocha": "^6.1.4",
    "nyc": "^14.0.0",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "rollup": "^1.10.1",
    "rollup-plugin-babel": "^4.3.2",
    "rollup-plugin-commonjs": "^9.3.4",
    "rollup-plugin-node-builtins": "^2.1.2",
    "rollup-plugin-node-globals": "^1.4.0",
    "rollup-plugin-node-resolve": "^4.2.3",
    "rollup-plugin-uglify": "^6.0.2",
    "standard-version": "^5.0.2",
    "tslint": "^5.16.0",
    "tslint-config-semistandard": "^8.0.0",
    "typedoc": "^0.14.2",
    "typescript": "^3.4.5",
    "validate-commit-msg": "^2.14.0",
    "webpack": "^4.30.0",
    "webpack-assets-manifest": "^3.1.1",
    "webpack-cli": "^3.3.1"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-conventional-changelog"
    },
    "validate-commit-msg": {
      "types": "conventional-commit-types",
      "helpMessage": "Use \"git cz\" or conventional-changelog format :) (https://github.com/commitizen/cz-cli)"
    }
  },
  "husky": {
    "hooks": {
      "commit-msg": "validate-commit-msg"
    }
  }
}
